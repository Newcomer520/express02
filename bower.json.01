{
  "name": "expression2",
  "version": "0.0.1",
  "main": "_public/js/app.js",
  "homepage": "https://github.com/Newcomer520/express02",
  "authors": [
    "Newcomer <newcomer521@gmail.com>"
  ],
  "description": "express + bower + gulp",
  "moduleType": [
    "node"
  ],
  "keywords": [
    "bower"
  ],
  "license": "MIT",
  "ignore": [
    "**/.*",
    "node_modules",
    "bower_components",
    "test",
    "tests"
  ],
  "dependencies": {
    "angular": "~1.2.16",
    "bootstrap": "~3.1.1",
    "jquery": ">1.5"
  },
  "overrides": {
    "jquery": {
        "main": {
          "development": "./dist/jquery.js",
          "production": "./dist/jquery.min.js"
        }
    },
    "angular": {
        "main": {
          "development": "angular.js",
          "production": "angular.min.js"      
        }
    },
    "bootstrap": {
        "main": {
          "development": [
            "./dist/css/bootstrap.css",
            "./dist/js/bootstrap.js",
            "./dist/fonts/glyphicons-halflings-regular.eot",
            "./dist/fonts/glyphicons-halflings-regular.svg",
            "./dist/fonts/glyphicons-halflings-regular.ttf",
            "./dist/fonts/glyphicons-halflings-regular.woff"
          ],
          "production": [
            "./dist/css/bootstrap.css",
            "./dist/js/bootstrap.min.js",
            "./dist/fonts/glyphicons-halflings-regular.eot",
            "./dist/fonts/glyphicons-halflings-regular.svg",
            "./dist/fonts/glyphicons-halflings-regular.ttf",
            "./dist/fonts/glyphicons-halflings-regular.woff"
          ]
        }
    }
  }
}
